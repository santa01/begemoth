{
    "commands": {
        "help": {
            "responses": [
                "Sorry {USERNAME}, no help available.",
                "God help us all {USERNAME}."
            ]
        }
    },

    "extended_commands": {
        "help": {
            "responses": [
                "Sorry {USERNAME}, I cannot help {ARGUMENT}.",
                "{ARGUMENT}? Dunno what are you talking about."
            ]
        }
    },

    "unknown_commands": {
        "responses": [
            "Unknown command {COMMAND}.",
            "Nothing to do with {COMMAND}."
        ]
    },

    "plugins": {
        "bash": {
            "responses": [
                "/me is reading a quote:\n\n{OUTPUT}",
                "Quote of the day:\n\n{OUTPUT}"
            ]
        },

        "roll": {
            "responses": [
                "/me is throwing a dice:\n\n{OUTPUT}",
                "The voices in my head keep telling me:\n\n{OUTPUT}"
            ]
        },

        "jukebox": {
            "responses": [
                "Now playing: {OUTPUT}",
                "Next track is {OUTPUT}"
            ]
        }
    },

    "events": {
        "user_online": {
            "responses": [
                "Hello {USERNAME}!",
                "Hola {USERNAME}!"
            ]
        },

        "self_online": {
            "responses": [
                "Hello everyone!",
                "/me is back!"
            ]
        },

        "user_offline": {
            "responses": [
                "Bye {USERNAME}!",
                "Farewell {USERNAME}!"
            ]
        }
    }
}
